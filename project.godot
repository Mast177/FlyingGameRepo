; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[animation]

warnings/check_invalid_track_paths=false

[application]

config/name="Flying Game"
run/main_scene="res://Scenes/Menus/main_menu.tscn"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://Assets/Scripts/ManagerScripts/game_manager.gd"
SignalBus="*res://Assets/Scripts/ManagerScripts/signal_bus.gd"
PlayerStats="*res://Assets/Scripts/ManagerScripts/player_stats.gd"
SceneTransitionController="*res://Assets/Transitions/SceneTransitionController.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/AS2P/plugin.cfg")

[global_group]

main="This group is dedicated to the main node in a level scene. This is so that spawning scenes know which node is going to be their parent."
player_projectiles="projectiles given this tag will not deal damage to the player"
player="If a node is part of this group, then projectiles in the \"player_projectiles\" group will deal no damage"
enemy="a group designated for enemies for identifying what projectiles hit"
UI="This group is designated to the UI so that it can easily be found by other nodes & scenes which need to access its variables. (For setting score, for example)"
Edge="This group is assigned to nodes which act as the edges of the game world. Used for detecting bounces off edges of game area."
spawn_point="This group is assigned to any node which is classified as an enemy spawnpoint for the purpose of identifying where an enemy is supposed to spawn"
enemy_projectiles="a group specifically for projectiles belonging to enemies"
Explosion="This group is designated for Hitboxes which are explosions"

